<Window x:Class="Profess.Day6.WindowDockPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Profess.Day6"
        mc:Ignorable="d"
        Title="WindowDockPanel" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="False">
            <Button DockPanel.Dock="Left" Width="60"/>
            <Button DockPanel.Dock="Bottom" Height="60"/>
            <Button DockPanel.Dock="Right" Width="150"/>
            <Button DockPanel.Dock="Top" Height="150"/>
            <Button DockPanel.Dock="Left" Width="100"/>
        </DockPanel>
    </Grid>
</Window>
